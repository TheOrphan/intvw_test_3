    "bowser": "^2.10.0",
    "dotenv": "^8.2.0",
    "less": "^3.10.3",
    "less-vars-to-js": "^1.3.0",
    "next": "^9.4.4",
    "null-loader": "^3.0.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-number-format": "^4.4.1"
  },
  "devDependencies": {
    "cross-env": "^7.0.0",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.2",
    "workbox-window": "^5.1.3"
  }
